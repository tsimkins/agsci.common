<tal:css 
    tal:define="portal_url view/portal_url|none;
                editing view/editing|nothing;
                faceted_enabled view/faceted_enabled|nothing">
    
    <script type="text/javascript"
        tal:condition="faceted_enabled"
        data-bundle="faceted-jquery">
        jq(document).ready(function(evt){
            Faceted.Load(evt, 'http://trs22.psu.edu:5051/test/degree-explorer/');
        });

        jq(window).on('unload', function(){
            Faceted.Unload();
        });
        
    </script>

    
</tal:css>