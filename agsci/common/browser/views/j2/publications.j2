{% for _ in data %}
<p class="publication mb-5">
    {% if _.doi %}<a href="{{_.doi}}">{% endif %}<span class="publication-title">{{ _.title }}</span>{% if _.doi %}</a>{% endif %}<br />{% if _.journal_title %}<span class="journal-name">{{_.journal_title}}</span>{% endif %}{% if _.contributors %}, {{ ", ".join(_.contributors) }}{% endif %}{% if _.published_on %}, {{_.published_on.year}}{% endif %}

</p>
{% endfor %}