<?xml version="1.0"?>
<registry>

    <!-- criteria widget -->
    <records prefix="plone.resources/faceted-widgets-criteria-degree-explorer-view"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++eea.facetednavigation.widgets.criteria_degree_explorer.view.js</value>
    </records>

    <records prefix="plone.resources/faceted-widgets-checkbox-degree-explorer-view"
             interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++eea.facetednavigation.widgets.checkbox_degree_explorer.view.js</value>
    </records>

