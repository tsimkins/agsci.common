               interface='Products.CMFPlone.interfaces.IBundleRegistry'>
        <value key="resources">
          <element>faceted-navigation-view</element>
          <element>faceted-navigation-expand</element>
          <element>faceted-navigation-independent</element>
          <element>faceted-widgets-alphabets-view</element>
          <element>faceted-widgets-autocomplete-view</element>
          <element>faceted-widgets-checkbox-view</element>
          <element>faceted-widgets-checkbox-degree-explorer-view</element>
          <element>faceted-widgets-criteria-view</element>
          <element>faceted-widgets-criteria-degree-explorer-view</element>
          <element>faceted-widgets-date-view</element>
          <element>faceted-widgets-daterange-view</element>
          <element>faceted-widgets-debug-view</element>
          <element>faceted-widgets-etag-view</element>
          <element>faceted-widgets-multiselect-view</element>
          <element>faceted-widgets-path-tree</element>
          <element>faceted-widgets-path-view</element>
          <element>faceted-widgets-portlet-view</element>
          <element>faceted-widgets-radio-view</element>
          <element>faceted-widgets-range-view</element>
          <element>faceted-widgets-resultsfilter-view</element>
          <element>faceted-widgets-resultsperpage-view</element>
          <element>faceted-widgets-select-view</element>
          <element>faceted-widgets-sorting-view</element>
          <element>faceted-widgets-tagscloud-view</element>
          <element>faceted-widgets-tal-view</element>
          <element>faceted-widgets-text-view</element>
        </value>
    </records>
    <!-- CSS file for TinyMCE -->
    <record name="plone.content_css"
            interface="Products.CMFPlone.interfaces.controlpanel.ITinyMCESchema"
            field="content_css">
        <value>
            <element>++plone++static/components/tinymce-builded/js/tinymce/skins/lightgray/content.min.css</element>
            <element>++resource++agsci.common/assets/css/tinymce.css</element>
        </value>
    </record>

    <!-- JSON config for TinyMCE to make the <object> tags work. -->
    <record name="plone.other_settings"
            interface="Products.CMFPlone.interfaces.controlpanel.ITinyMCESchema"
            field="other_settings">

        <value>
            {
                "forced_root_block" : ""
            }
        </value>

    </record>

    <!-- Heading styles -->
    <record name="plone.header_styles"
            interface="Products.CMFPlone.interfaces.controlpanel.ITinyMCESchema"
            field="header_styles">

        <value>
            <element>Header 1|h2</element>
            <element>Header 2|h3</element>
            <element>Header 3|h4</element>
            <element>Header 4|h5</element>
        </value>

    </record>

    <!-- Inline styles -->
    <record
        name="plone.inline_styles"
        interface="Products.CMFPlone.interfaces.controlpanel.ITinyMCESchema"
        field="inline_styles">
        <value>
            <element>Bold|bold|bold</element>
            <element>Italic|italic|italic</element>
            <element>Code|code|code</element>
        </value>
    </record>

    <!-- Alignment Styles -->

    <record name="plone.alignment_styles"
        interface="Products.CMFPlone.interfaces.controlpanel.ITinyMCESchema"
        field="alignment_styles">
        <value>
            <element>Left|alignleft|alignleft</element>
            <element>Right|alignright|alignright</element>
        </value>
    </record>

    <record name="plone.app.tiles">
        <value purge="false">
