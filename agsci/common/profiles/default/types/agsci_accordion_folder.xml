<?xml version="1.0" encoding="UTF-8"?>
<object xmlns:i18n="http://xml.zope.org/namespaces/i18n" name="agsci_accordion_folder" meta_type="Dexterity FTI" i18n:domain="plone">
    <property name="title" i18n:translate="">Accordion Folder</property>
    <property name="description" i18n:translate="" />
